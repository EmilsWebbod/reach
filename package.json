{
  "name": "@ewb/reach",
  "version": "0.2.5",
  "description": "Resource and Fetch stuff",
  "type": "module",
  "engines": {
    "node": "14.20.0"
  },
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "start": "yarn build:watch",
    "build": "tsc --incremental",
    "build:watch": "tsc --watch --incremental",
    "lint": "tslint --project \"./tsconfig.json\"",
    "test": "mocha --watch --watch-extensions ts -r ts-node/register test/**/*.spec.ts",
    "test:active": "yarn test --watch -g active:",
    "storybook": "start-storybook -p 9009 -s public",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EmilsWebbod/reach.git"
  },
  "keywords": [
    "typescript",
    "ts",
    "reach",
    "fetch",
    "crud",
    "api"
  ],
  "author": "Emil Andreas Olsen <emil@webbod.no>",
  "bugs": {
    "url": "https://github.com/EmilsWebbod/reach/issues"
  },
  "homepage": "https://github.com/EmilsWebbod/reach#readme",
  "files": [
    "types",
    "utils",
    "index.ts",
    "index.d.ts",
    "index.js",
    "core"
  ],
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.3",
    "prettier": "^2.7.1",
    "tslint": "^5.11.0",
    "typescript": "^4.8.3",
    "vitest": "^0.23.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
